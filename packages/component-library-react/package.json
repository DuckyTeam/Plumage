{
  "name": "@ducky/plumage-react",
  "sideEffects": false,
  "version": "1.1.2",
  "description": "Plumage is Ducky's design system; this is React wrapper",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DuckyTeam/Plumage.git"
  },
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "clean": "rm -rf dist",
    "build:esm": "tsc -p . --module es2015 --outDir dist/esm",
    "build:cjs": "tsc -p . --module commonjs --outDir dist/cjs"
  },
  "main": "./dist/cjs/index.js",
  "types": "./dist/esm/index.d.ts",
  "module": "./dist/esm/index.js",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@types/node": "^16.11.19",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "np": "^3.1.0",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@ducky/plumage": "^3.0.0"
  },
  "peerDependencies": {
    "react": ">=16.7",
    "react-dom": ">=16.7"
  }
}
